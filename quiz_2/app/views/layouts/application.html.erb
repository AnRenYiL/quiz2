<!DOCTYPE html>
<html>
  <head>
    <title>Quiz2</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all' %>
    <%= javascript_pack_tag 'application' %>
  </head>

  <body class="container">
    <% flash.each do |type, message|%>
    <div class="flush flush-<%= type%>">
      <%= message%>
    </div>
    <% end%>
    <%= yield %>
  </body>
</html>
